{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "default": "product name"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "previousPrice": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "isNew": {
      "type": "boolean"
    },
    "brand": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "min": 1,
      "max": 1,
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "description": {
      "type": "text"
    }
  }
}
